{
  "inlay_hints": {
    "enabled": true,
    "show_value_hints": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "show_background": false,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50,
    "toggle_on_modifiers_press": {
      "control": false,
      "alt": false,
      "shift": false,
      "platform": false,
      "function": false
    }
  },
  "buffer_font_features": {
    "calt": 1
  },
  "ui_font_family": ".SystemUIFont",
  "buffer_font_family": "Maple Mono",
  "base_keymap": "VSCode",
  "minimap": {
    "show": "never"
  },
  "autosave": "on_focus_change",
  "telemetry": {
    "diagnostics": true,
    "metrics": false
  },
  "vim_mode": true,
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Colored Zed Icons Theme Dark"
  },
  "auto_update": false,
  "bottom_dock_layout": "contained",
  "close_on_file_delete": true,
  "disable_ai": true,
  "load_direnv": "shell_hook",
  "scrollbar": {
    "diagnostics": "warning"
  },
  "tabs": {
    "git_status": true
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/.jj",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    "**/.envrc"
  ],
  "git": {
    "inline_blame": {
      "enabled": false
    },
    "hunk_style": "unstaged_hollow"
  },
  "indent_guides": {
    "active_line_width": 2
  },
  "hover_popover_delay": 200,
  "image_viewer": {
    "unit": "decimal"
  },
  "journal": {
    "path": "~/Documents",
    "hour_format": "hour24"
  },
  "line_indicator_format": "short",
  "middle_click_paste": false,
  "multi_cursor_modifier": "cmd_or_ctrl",
  "redact_private_values": true,
  "relative_line_numbers": true,
  "when_closing_with_no_tabs": "close_window",
  "title_bar": {
    "show_branch_icon": false,
    "show_branch_name": false,
    "show_project_items": false,
    "show_onboarding_banner": false,
    "show_user_picture": false,
    "show_sign_in": false,
    "show_menus": false
  },
  "terminal": {
    "button": true,
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "project_panel": {
    "dock": "left",
    "entry_spacing": "standard",
    "scrollbar": {
      "show": "never"
    },
    "hide_root": true,
    "starts_open": false
  },
  "git_panel": {
    "button": true,
    "dock": "left",
    "default_width": 460,
    "status_style": "icon",
    "fallback_branch_name": "main",
    "sort_by_path": false,
    "collapse_untracked_diff": true,
    "scrollbar": {
      "show": "never"
    }
  },
  "outline_panel": {
    "button": true,
    "default_width": 300,
    "dock": "left",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 12,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true,
    "indent_guides": {
      "show": "always"
    },
    "scrollbar": {
      "show": "never"
    }
  },
  "calls": {
    "mute_on_join": true,
    "share_on_join": false
  },
  "debugger": {
    "stepping_granularity": "line",
    "save_breakpoints": true,
    "dock": "bottom",
    "button": true
  },
  "profiles": {
    "zen-dark": {
      "theme": "Gruvbox Dark"
    },
    "zen-light": {
      "theme": "Gruvbox Light Hard"
    },
    "presentation": {
      "buffer_font_size": 16,
      "ui_font_size": 16,
      "title_bar": {
        "show_branch_icon": false,
        "show_branch_name": false,
        "show_project_items": false,
        "show_onboarding_banner": false,
        "show_user_picture": false,
        "show_sign_in": false,
        "show_menus": false
      },
      "toolbar": {
        "breadcrumbs": false,
        "quick_actions": false,
        "selections_menu": false,
        "agent_review": false,
        "code_actions": false
      },
      "theme": "One Light"
    }
  },
  "ui_font_size": 12,
  "buffer_font_size": 10,
  "theme": "Gruvbox Dark"
}
